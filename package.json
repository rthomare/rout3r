{
  "name": "rout3r",
  "version": "0.15.0",
  "homepage": "https://rthree.xyz",
  "author": {
    "name": "Rohan Thomare",
    "url": "https://github.com/rthomare"
  },
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "./dev.sh",
    "add-domain": "echo \"rthree.xyz\" > dist/CNAME",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "deploy": "./deploy.sh",
    "lint": "eslint src --ext .tsx --ext .ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --config ./.prettierrc -w 'src/**/*.{tsx,ts}' && git update-index --again",
    "pretest": "yarn lint:fix",
    "test": "vitest --dom",
    "posttest": "npx http-server coverage/lcov-report",
    "test:watch": "yarn test --collectCoverage=false --watch",
    "prepare": "husky install",
    "cz": "cz"
  },
  "dependencies": {
